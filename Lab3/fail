[032mint main () {
	int n, i;
	n = readInt();
	i = 2;
	while (i <= n) {
		bool iPrime = true;
		int j = 2;
		while (j*j <= i && iPrime) {
			if ((i / j) * j == i) {
				iPrime = false;
			} else {}
			j++;
		}

		if (iPrime && (n / i) * i == n) {
			printInt(i);
			n = n / i;
		} else { 
			i++; 
		}
	}

